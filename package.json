{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "build": "next build",
    "serve": "next start",
    "lint:scripts:ci": "eslint . --ext .ts,.tsx",
    "lint:scripts": "npm run lint:scripts:ci -- --fix",
    "lint:format:ci": "prettier . -c --ignore-path .gitignore",
    "lint:format": "npm run lint:format:ci -- -w",
    "lint:ci": "npm run lint:scripts:ci && npm run lint:format:ci",
    "lint": "npm run lint:scripts && npm run lint:format",
    "type-check": "tsc",
    "validate:ci": "npm run lint:ci && npm run type-check",
    "validate": "npm run lint && npm run type-check",
    "setup:ci": "npm ci && npm run validate:ci",
    "setup": "npm ci && npm run validate",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{tsx,ts,css}": [
      "npm run lint"
    ]
  },
  "engines": {
    "node": "^14.0.0"
  },
  "dependencies": {
    "focus-visible": "^5.2.0",
    "modern-normalize": "^1.1.0",
    "next": "^13.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-next": "^13.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.9.3"
  }
}
